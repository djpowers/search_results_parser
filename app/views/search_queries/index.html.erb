<%= link_to 'Home', root_path %>

<table>
  <thead>
    <tr>
      <th>Search Query</th>
      <th>Total Searches</th>
    </tr>
  </thead>

  <tbody>
    <% @search_queries.each do |query| %>
      <tr>
        <td><%= link_to "#{query.terms}", search_query_path(query) %></td>
        <td><%= query.result_groups.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
